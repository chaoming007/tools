$(function(){

/*
 *   图表展示数据
*/
 var data=[
    {
        name:"03月",
        score:52
    },
    {
        name:"04月",
        score:380
    },

    {
        name:"05月",
        score:4753
    }
    
];  

 var data1=[
    {
        name:"03月",
        score:5200
    },
    {
        name:"04月",
        score:380
    },

    {
        name:"05月",
        score:4753
    },
     {
        name:"04月",
        score:3800
    },
     {
        name:"04月",
        score:8080
    }
    
]; 

/*
 * 图表插件
 * maxNum：图表显示的最大值，如果数据值超过图表最大值时将会超出图表
 * datArr：数据必须值不能为空 例如：[{name:"2016/03月", score:52}] 
 * lineNum：背景标尺线的数量，默认为3条
 * tabHeight：图表的高度，默认为100像素
 * moveType：运动类型，默认为Bounce.easeOut
 * 类型包(Linear,Quad.easeIn,Quad.easeOut,Quad.easeInOut...)
 */



$("#drawCanvas").charTab({
        maxNum:10000,                   //最大值
        datArr:data,                   //数据
        lineNum:3,                    //背景线数量
        tabHeight:300,                //图形高度
        moveType:"Bounce.easeOut" //运动类型
});



 $("#drawCanvas1").charTab({
    maxNum:10000,                   //最大值
    datArr:data1,                   //数据
    lineNum:5,                    //背景线数量
    tabHeight:80,                //图形高度
    moveType:"Bounce.easeOut" //运动类型
 });









})