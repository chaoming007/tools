/*页面*/

.index {
  position: relative;
  width: 100%;
  display: block;
  background-color: #fff1ee;
}

.index:before {
  content: '';
  position: fixed;
  width: 92px;
  height: 61px;
  background-image: url(../img/public.png);
  background-size: 250px auto;
  background-repeat: no-repeat;
  background-position: -85px 0px;
  left: 0;
  bottom: 0;
}

.index .full-screen:before {
  position: absolute;
  content: '';
  width: 39px;
  height: 49px;
  background-image: url(../img/public.png);
  background-size: 250px auto;
  background-position: -206px -2px;
  background-repeat: no-repeat;
  bottom: -36px;
  right: 0px;
}

.index .full-screen .lnk-hot {
  position: absolute;
  width: 69px;
  height: 42px;
  background-image: url(../img/hot.png);
  background-size: 69px auto;
  background-repeat: no-repeat;
  right: 10px;
  top: 10px;
}

.index .content {
  width: 100%;
  display: block;
  position: relative;
}

.index .content .swiper {
  position: relative;
  height: 180px;
}

.index .content .swiper .swiper-container {
  position: absolute;
  width: 100%;
  top: -57px;
}

.index .content .swiper:before {
  position: absolute;
  content: '';
  width: 209px;
  height: 98px;
  background-image: url(../img/flower.png);
  background-repeat: no-repeat;
  background-size: 209px auto;
  left: 50%;
  margin-left: -105px;
